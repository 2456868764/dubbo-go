{
  "version": "app1_version_1",
  "rules": {
    "action": "DENY",
    "policies": {
      "header-regex-match": {
        "permissions": [
          {
            "any": true
          }
        ],
        "principals": [
          {
            "orIds": {
              "ids": [
                {
                  "header": {
                    "name": ":path",
                    "string_match": {
                      "safe_regex": {
                        "google_re2": {},
                        "regex": ".*/deny.*"
                      }
                    }
                  }
                },
                {
                  "header": {
                    "name": ":method",
                    "exactMatch": "HEAD"
                  }
                },
                {
                  "header": {
                    "name": ":path",
                    "prefixMatch": "/control-api"
                  }
                },
                {
                  "header": {
                    "name": ":path",
                    "suffixMatch": ".html"
                  }
                },
                {
                  "header": {
                    "name": "X-Timeout",
                    "rangeMatch": {
                      "start": 100,
                      "end": 999999
                    }
                  }
                }
              ]
            }
          }
        ]
      }
    }
  },
  "shadowRules": {}
}
